{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "preserve",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "@rc-component/dialog": [
        "src/index.ts"
      ]
    },
   "types": ["node", "jest", "@testing-library/jest-dom"],
  },
  "include": [
    ".dumirc.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./docs/**/*.tsx",
    "./tests/**/*.tsx"
  ]
}