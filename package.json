{
  "name": "rc-dialog",
  "version": "4.0.4",
  "description": "dialog ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-dialog",
    "dialog",
    "ui"
  ],
  "homepage": "http://github.com/react-component/dialog",
  "maitainers": [
    "yiminghe@gmail.com",
    "dxq613@gmail.com"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/dialog.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/dialog/issues"
  },
  "licenses": "MIT",
  "spm": {
    "buildArgs": "--global react:window.React",
    "dependencies": {
      "react": "*"
    }
  },
  "config": {
    "port": 8000
  },
  "scripts": {
    "build": "rc-tools run build",
    "less": "rc-tools run less",
