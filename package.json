    "browser-test": "rc-tools run browser-test",
    "browser-test-cover": "rc-tools run browser-test-cover"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "bootstrap": "^3.3.2",
    "expect.js": "~0.3.1",
    "jquery": "^1.11.2",
    "precommit-hook": "^1.0.7",
    "rc-server": "^2.0.0",
    "rc-tools": "2.x",
    "react": "~0.13.0"
  },
  "precommit": [
    "lint",
    "less"
  ],
  "dependencies": {
    "dom-align": "~1.0.3",
    "object-assign": "~2.0.0",
    "rc-util": "~2.0.2"
